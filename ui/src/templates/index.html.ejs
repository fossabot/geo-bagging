<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <title>GeoBagging</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <%if (baseUrl != undefined) { %>
            <script type="text/javascript">
                window.geoBaggingBaseUrl = '<%= baseUrl %>'
            </script>
        <% } %>
        <script type="text/javascript">
            function showError(errorMessage) {
                document.write(errorMessage)
            }
            <%= require('raw-loader!./feature-detect.js').default %>
        </script>
        <script type="text/javascript">
            <%= require('raw-loader!./error_handler.js').default %>
        </script>
        <!-- stylesheets -->
    </head>
    <body>
        <%= require('raw-loader!./error-display.html').default %>
        <%= require('raw-loader!./loading.html').default %>
        <!--scripts-->
    </body>
</html>
